<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data noupdate="0">

    <record model="ir.module.category" id="quickship_category_user">
        <field name="name">QuickShip User</field>
        <field name="description">A user with access to the QuickShip module.</field>
    </record>

    <record id="group_quickship_shipper" model="res.groups">
        <field name="name">Shipper</field>
        <field name="category_id" ref="quickship_category_user"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_quickship_picker" model="res.groups">
        <field name="name">Picker</field>
        <field name="category_id" ref="quickship_category_user"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_quickship_packer" model="res.groups">
        <field name="name">Packer</field>
        <field name="category_id" ref="quickship_category_user"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_quickship_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="quickship_category_user"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

</data>
</openerp>
